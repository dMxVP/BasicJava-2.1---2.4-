public class Poetry extends ConsoleProgram
{
    public void run()
    {
      System.out.println("Rossees are red,");
      System.out.println("Violets are blue,");
      System.out.println("Haiku's greate.");
      System.out.println("Rossees are red,");
      System.out.println("Violets are blue,");
      System.out.println("Haiku's greate,");
      System.out.println("But Acrostics is grater");
      System.out.println("Because it has 5 lines.");
      String poem = readLine("Write your poem here:");
      System.out.println(poem);
    
    }
}
